{
  "name": "createjs-particle-waypoint",
  "version": "0.3.6",
  "description": "Particle waypoint module for createjs.",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "repository": "https://MasatoMakino@github.com/MasatoMakino/createjs-particle-waypoint.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@types/easeljs": "^1.0.0",
    "@types/tweenjs": "^1.0.0",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.7",
    "dat.gui": "^0.7.6",
    "glob": "^7.1.4",
    "husky": "^3.0.7",
    "prettier": "^1.18.2",
    "replace-in-file": "^4.1.3",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "peerDependencies": {
    "easeljs": "^1.0.2",
    "tweenjs": "^1.0.2"
  },
  "dependencies": {
    "particle-waypoint": "https://github.com/MasatoMakino/particle-waypoint.git"
  },
  "scripts": {
    "build": "npm run prettier:src && tsc && npm run babel && npm run doc",
    "prettier:src": "prettier --write './!(bin|docs|coverage)/**/*.{js,ts,json}' ",
    "watch": "npm run watch:tsc | npm run watch:babel | npm run watch:server ",
    "watch:tsc": "tsc -w",
    "babel": "webpack --mode development",
    "watch:babel": "npm run babel -- -w",
    "watch:server": "node task/browsersync.js development",
    "server": "node task/browsersync.js production",
    "doc": "npm run doc:generate && npm run doc:replaceNode && npm run doc:card",
    "doc:generate": "typedoc --tsconfig tsdocconfig.json src",
    "doc:replaceNode": "node ./task/replaceAPI.js",
    "doc:card": "repoimg -n MasatoMakino/createjs-particle-waypoint -o ./docs/card.png"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:src"
    }
  }
}
